-- daily
-- add to pokemon_history
insert ignore into mons.pokemon_history select * from mons.pokemon_history_temp where date(CONVERT_TZ(last_modified, '+00:00', @@global.time_zone)) = curdate() - interval 1 day;

-- delete from pokemon_history_temp
delete from mons.pokemon_history_temp where date(CONVERT_TZ(last_modified, '+00:00', @@global.time_zone)) = curdate() - interval 1 day;
